   "source": [
    "def gen(a):\n",
    "    sr = [2 * i + 1 for i in range(a)]\n",
    "    return sr\n",
    "\n",
    "a = int(input(\"Enter a number: \"))\n",
    "res = gen(a)\n",
    "print(\", \".join(map(str, res)))"
